@prefix ex:   <https://example.org/dataset/> .
@prefix dcat: <https://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

##########################################################
# DCAT Dataset metadata – Kaggle Heart Disease Indicators
##########################################################
ex:heart-disease-2022 a dcat:Dataset ;
  dct:title "Personal Key Indicators of Heart Disease 2022" ;
  dct:description "Dataset derived from the 2022 CDC Behavioral Risk Factor Surveillance System (BRFSS), containing key health indicators related to heart disease, as published on Kaggle." ;
  dct:creator "Centers for Disease Control and Prevention (CDC)" ;
  dct:publisher "Kaggle user: Kamil Pytlak" ;
  dct:issued "2022-12-31"^^xsd:date ;
  dct:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
  dcat:distribution ex:heart-disease-2022-csv , ex:heart-disease-2022-rdf .

##############################################################
# Distribution 1: Original dataset (non-RDF) – Kaggle version
##############################################################
ex:heart-disease-2022-csv a dcat:Distribution ;
  dct:title "Heart Disease Indicators 2022 (CSV, Kaggle version)" ;
  dcat:mediaType "text/csv" ;
  dcat:downloadURL <https://www.kaggle.com/datasets/kamilpytlak/personal-key-indicators-of-heart-disease> ;
  prov:wasDerivedFrom <https://www.cdc.gov/brfss/annual_data/annual_2022.html> ;
  dct:license <https://creativecommons.org/publicdomain/zero/1.0/> .

###########################################################
# Distribution 2: RDF (FAIRified) – dummy for now
###########################################################
ex:heart-disease-2022-rdf a dcat:Distribution ;
  dct:title "Heart Disease Indicators 2022 (FAIRified RDF version)" ;
  dcat:mediaType "text/turtle" ;
  dcat:downloadURL <https://example.org/data/heart-2022.ttl> ;
  dct:conformsTo <https://www.w3.org/TR/vocab-dcat-3/> ;
  dct:issued "2025-10-15"^^xsd:date .
